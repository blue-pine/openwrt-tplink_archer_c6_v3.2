config rule 'LAN_ON'
    option ledname 'LAN'
    option ledon '1'

config rule 'LAN_OFF'
    option ledname 'LAN'
    option ledon '0'

config rule 'USB'
    option ledname 'USB'
    option ledon '0'

config rule 'WAN0_ON'
    option ledname 'WAN1'
    option ledon '1'

config rule 'WAN0_OFF'
    option ledname 'WAN1'
    option ledon '0'

