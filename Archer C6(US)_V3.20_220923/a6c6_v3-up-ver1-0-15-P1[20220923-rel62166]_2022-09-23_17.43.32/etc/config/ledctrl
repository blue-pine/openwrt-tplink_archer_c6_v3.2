config rule 'STATUS_ON'
    option ledname 'STATUS'
    option ledon '1'

config rule 'STATUS_OFF'
    option ledname 'STATUS'
    option ledon '0'

config rule 'STATUS_SAN'
    option ledname 'STATUS'
    option delayon '350'
    option delayoff '350'
    option duration '600000'

config rule 'WPS_ON'
    option ledname 'WPS'
    option ledon '1'

config rule 'WPS_OFF'
    option ledname 'WPS'
    option ledon '0'

config rule 'WPS_SUCCESS'
    option ledname 'WPS'
    option delayon '300000'
    option delayoff '0'
    option duration '300000'

config rule 'WPS_ERROR'
    option ledname 'WPS'
    option delayon '250'
    option delayoff '250'
    option duration '60000'

config rule 'WPS_OVERLAP'
    option ledname 'WPS'
    option delayon '250'
    option delayoff '250'
    option delaylast '2000'
    option interval '500'
    option duration '30000'

config rule 'WPS_INPROGRESS'
    option ledname 'WPS'
    option delayon '2000'
    option delayoff '1000'
    option duration '120000'
